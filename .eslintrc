{
  "extends": ["eslint:recommended"],
  "plugins": [],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2015
  },
  "env": {
    "browser": true,
    "jquery": true
  },
  "globals": {
    "Promise": false,
    "logging": false,
    "isString": false,
    "_CONTEXT": false
  },
  "rules": {
    "no-console": "off",
    "quotes": ["warn", "double"]
  }
}
